                <button class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
              </div>
            </div><!-- /.box-header -->
            <div class="box-body">
              <div class="row">
                <div class="col-md-8">
                  <div class="chart-responsive">
                    <canvas id="pieChart" height="150"></canvas>
                  </div><!-- ./chart-responsive -->
                </div><!-- /.col -->
                <div class="col-md-4">
                  <ul class="chart-legend clearfix">
                    <li><i class="fa fa-circle-o text-blue"></i> Hombres</li>
                    <li><i class="fa fa-circle-o text-pink"></i> Mujeres</li>
                  </ul>
                </div><!-- /.col -->
              </div><!-- /.row -->
            </div><!-- /.box-body -->
            {% for c in customers %}
              {{ "now"|date('Y-m-d') - c.birthday }}
            {% endfor %}
            <div class="box-footer no-padding">
              <ul class="nav nav-pills nav-stacked">
                <li><a href="#">Niños <span class="pull-right text-red"></i>  </span></a></li>
                <li><a href="#">Jovenés <span class="pull-right text-green">  </span></a></li>
                <li><a href="#">Adultos <span class="pull-right text-yellow">  </span></a></li>
                <li><a href="#">Ancianos <span class="pull-right text-yellow">  </span></a></li>
              </ul>
            </div><!-- /.footer -->
          </div><!-- /.box -->
        </div><!-- /.col -->
      </div><!-- /.row -->

      <div class='row'>
        <div class='col-md-4'>
