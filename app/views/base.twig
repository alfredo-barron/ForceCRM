<!doctype html>
<html lang="es">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  {% block title %}{% endblock %}
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/pace-theme-minimal.css">
  <script data-pace-options='{ "ajax": false, "restartOnRequestAfter": false }' src="{{ urlFor('root') }}public/js/pace.min.js"></script>
  <link rel="shortcut icon" href="{{ urlFor('root') }}crm.png" type="image/x-icon">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/bootstrap/css/bootstrap.min.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/dist/css/AdminLTE.min.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/dist/css/skins/_all-skins.min.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/css/font-awesome-4.3.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/plugins/morris/morris.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/plugins/jvectormap/jquery-jvectormap-1.2.2.css">
  <link rel="stylesheet" href="{{ urlFor('root') }}public/plugins/daterangepicker/daterangepicker-bs3.css">
  {% block css %}{% endblock %}
</head>
<body class="skin-blue layout-boxed">
  {% block content %}{% endblock %}
  <!-- JS -->
    <script src="{{ urlFor('root') }}public/plugins/jQuery/jQuery-2.1.3.min.js"></script>
    <script src="{{ urlFor('root') }}public/css/bootstrap/js/bootstrap.min.js"></script>
    <!--<script src="{{ urlFor('root') }}public/plugins/fastclick/fastclick.min.js"></script>-->
    <script src="{{ urlFor('root') }}public/css/dist/js/app.min.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/sparkline/jquery.sparkline.min.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/datepicker/bootstrap-datepicker.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/iCheck/icheck.min.js"></script>
    <script src="{{ urlFor('root') }}public/plugins/slimScroll/jquery.slimscroll.min.js"></script>
  {% block js %}{% endblock %}
</body>
</html>
